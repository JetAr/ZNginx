     <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform float fWaves;
uniform float fHeight;
uniform float fSpeed;
uniform float fTimer;

varying vec4 vColor;

void main(void)
{
  
   float x = (gl_Vertex.x + fTimer * fSpeed) * fWaves;
   float fSin = sin(x);
   float height = fSin * fHeight;
   vec4 position = gl_Vertex;
   
   position.y += height;
   
   gl_Position = gl_ModelViewProjectionMatrix * position;
   vColor = (fSin + 1.0) * 0.5;
}]]></RmShaderCode></RmGLShader>
