attribute vec3 rm_Binormal;
attribute vec3 rm_Tangent;

varying vec3 vCameraDir;
   
void main( void )
{
   gl_Position = ftransform();
   gl_TexCoord[0] = gl_MultiTexCoord0;

   vec4 view_position = gl_ModelViewMatrix * gl_Vertex;
   vec3 camera_dir = normalize(view_position.xyz);
   
   mat3x3  TangentSpace;
   TangentSpace[0] = gl_NormalMatrix * rm_Tangent;
   TangentSpace[1] = gl_NormalMatrix * rm_Binormal;
   TangentSpace[2] = gl_NormalMatrix * gl_Normal;

   vCameraDir = camera_dir * TangentSpace;
   // equal to ....
   //vCameraDir.x = dot(camera_dir, TangentSpace[0]);
   //vCameraDir.y = dot(camera_dir, TangentSpace[1]);
   //vCameraDir.z = dot(camera_dir, TangentSpace[2]);
}]]></RmShaderCode></RmGLShader>
     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
varying vec3 vCameraDir;

uniform sampler2D DiffuseMap;
uniform sampler2D HeightMap;

uniform float fHeight;

void main( void )

{
   vCameraDir = normalize(vCameraDir);
   
   vec3  texcoord = vec3(gl_TexCoord[0].xy, fHeight);
   float depth = (1.0-texture2D(HeightMap, texcoord).r) * fHeight;
   vec3  texcoord_corrected = texcoord + vCameraDir.xyz * depth;
   
   gl_FragColor = texture2D(DiffuseMap, texcoord_corrected.xy); 
}]]></RmShaderCode></RmGLShader>
