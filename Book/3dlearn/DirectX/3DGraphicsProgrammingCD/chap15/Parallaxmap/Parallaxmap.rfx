     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
/*
   GLSL Improved Parallaxmap Shader
*/
varying vec3 vCameraDir;

uniform sampler2D DiffuseMap;
uniform sampler2D HeightMap;

uniform float fHeight;

void main( void )

{
   vCameraDir = normalize(vCameraDir);
   vec3 texcoord = vec3(gl_TexCoord[0].xy, fHeight);
   float height = 1.0f;
   float fStep = fHeight;
      
   for ( int i=0; i<10; i++ )
   {
      float height_next = texture2D(HeightMap, texcoord).r;
      float height_diff = height - height_next;
      texcoord += vCameraDir.xyz * height_diff * fStep;
      height = height_next;
      fStep *= 0.5;
   }
   
   gl_FragColor = texture2D(DiffuseMap, texcoord.xy); 
}]]></RmShaderCode></RmGLShader>
